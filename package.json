{
    "name": "url-scraper",
    "version": "1.0.0",
    "description": "Node.js CLI for scraping URLs from bracketed text with rate limiting and retry logic",
    "main": "index.js",
    "engines": {
        "node": ">=20.0.0"
    },
    "scripts": {
        "dev": "tsx watch src/cli.ts",
        "start": "tsx src/cli.ts",
        "build": "tsc -p tsconfig.json",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:cov": "vitest run --coverage",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "format": "prettier --write .",
        "prepare": "husky"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "commonjs",
    "lint-staged": {
        "*.{ts,tsx,js,cjs,mjs}": [
            "prettier --write",
            "eslint --max-warnings=0 --fix"
        ],
        "*.{md,mdx,json,yml,yaml}": [
            "prettier --write"
        ]
    },
    "devDependencies": {
        "@types/node": "^24.5.2",
        "@typescript-eslint/eslint-plugin": "^8.44.1",
        "@typescript-eslint/parser": "^8.44.1",
        "@vitest/coverage-v8": "^2.1.9",
        "eslint": "^9.36.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.3",
        "mocha": "^11.7.2",
        "prettier": "^3.6.2",
        "tsx": "^4.20.6",
        "typescript": "^5.9.2",
        "vitest": "^2.1.9"
    },
    "dependencies": {
        "cheerio": "^1.1.2",
        "dotenv": "^16.4.5"
    }
}
